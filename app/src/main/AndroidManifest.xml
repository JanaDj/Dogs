<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.meangirl.dogs">
    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activities.HomePage">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".GroupsList"/>
        <activity android:name=".Activities.DogPicker"/>
        <activity android:name=".Activities.BreedsList"/>
        <activity android:name=".Activities.AddDog"/>
        <activity android:name=".Activities.BreedInfo"/>
    </application>
</manifest>